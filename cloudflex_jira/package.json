{
  "name": "kr.osci.apps.flex_jira",
  "version": "0.0.1",
  "private": true,
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "scripts": {
    "build-jsx-browser": "parcel build --log-level 1 --global __root_component --out-dir views views/*.jsx",
    "build-jsx-node": "parcel build --log-level 1 --no-minify --target node --out-dir views/node views/*.jsx",
    "build": "concurrently -i 'npm:build-*'",
    "start": "NODE_ENV=development nodemon -r esm app.js",
    "watch-server": "nodemon -e js -r esm app.js",
    "watch-jsx-browser": "parcel watch --log-level 1 --no-hmr --global __root_component --out-dir views views/*.jsx",
    "watch-jsx-node": "parcel watch --log-level 1 --target node --out-dir views/node views/*.jsx",
    "watch-jsx": "concurrently 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "watch": "concurrently -i 'npm:watch-server' 'npm:watch-jsx-browser' 'npm:watch-jsx-node'",
    "lint": "eslint app.js routes"
  },
  "dependencies": {
    "@atlaskit/button": "^14.0.1",
    "@atlaskit/section-message": "^5.0.2",
    "atlassian-connect-express": "^4.3.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "errorhandler": "^1.5.1",
    "esm": "^3.2.22",
    "express": "^4.16.4",
    "express-hbs": "^2.3.0",
    "helmet": "^3.21.2",
    "https": "^1.0.0",
    "morgan": "^1.9.1",
    "moment": "^2.28.0",
    "nocache": "^2.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sequelize": "^5.21.2",
    "styled-components": "^5.1.1",
    "yarn": "^1.22.4",
    "cors": "^2.8.5",
    "fetch-headers": "^2.0.0",
    "form-data": "^3.0.0",
    "headers": "^0.9.6",
    "lodash": "^4.17.15",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.11",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^8.2.1",
    "uglify-js": "^2.8.29",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "atlassian-jwt": "^1.0.3",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.20.5",
    "longjohn": "^0.2.12",
    "ngrok": "^3.2.7",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-externals": "^0.5.1",
    "sqlite3": "^4.1.1"
  }
}
